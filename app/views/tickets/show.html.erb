<div id = "ticket">
  <header>
    <h1><%= @project.name %></h1>
  </header>
  <header>
    <h4><%= @ticket.name %></h4>
    <p><%= @ticket.description %></p>
    <ul class = "actions">
      <li><%= link_to "Edit Ticket", edit_project_ticket_path(@project,@ticket), class: "edit" %> </li>
      <li><%= link_to "Delete Ticket", project_ticket_path(@project,@ticket),
                      method: :delete,
                      data: { confirm: "Are you sure you want to delete this ticket?" },
                      class: "delete" %></li>
    </ul>
  </header>
</div>